{
  "name": "eslint-config-lifion",
  "version": "1.3.3",
  "description": "Lifion's ESLint config",
  "keywords": [
    "config",
    "eslint",
    "eslintconfig",
    "eslintrc",
    "javascript",
    "lifion",
    "styleguide"
  ],
  "author": "Edgardo Avilés <Edgardo.Aviles@ADP.com>",
  "maintainers": [
    "Chen Doron <Chen.Doron@ADP.com>",
    "Edgardo Avilés <Edgardo.Aviles@ADP.com>",
    "Jenny Eckstein <Jenny.Eckstein@ADP.com>",
    "Zaid Masud <Zaid.Masud@ADP.com>"
  ],
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lifion/eslint-config-lifion.git"
  },
  "bugs": {
    "url": "https://github.com/lifion/eslint-config-lifion/issues"
  },
  "homepage": "https://github.com/lifion/eslint-config-lifion#readme",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8.6.0"
  },
  "scripts": {
    "eslint": "eslint . --ext .js,.json --ignore-pattern='!.*.*'",
    "format": "prettier --write '**/*.{md,js,json}' '!coverage/**/*.{js,json}'",
    "prepare": "check-engines",
    "test": "jest -c ./.jest.json",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-es6-recommended": "^0.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-jsdoc": "^17.1.1",
    "eslint-plugin-json-light": "^1.0.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "eslint-plugin-sort-keys-fix": "^1.0.1",
    "eslint-plugin-unicorn": "^12.1.0"
  },
  "peerDependencies": {
    "eslint": "^5.16.0 || ^6.1.0",
    "prettier": ">=1.19.1"
  },
  "devDependencies": {
    "auto-changelog": "^1.16.2",
    "check-engines": "^1.5.0",
    "codecov": "^3.6.1",
    "eslint": "^6.6.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run eslint && npm test"
    }
  },
  "@lifion/core-commons": {
    "autoDocs": false,
    "template": "public",
    "updated": "2019-11-11T21:11:42.541Z",
    "version": "2.3.4"
  }
}
